{
  "name": "omnibar-www",
  "version": "0.0.0",
  "description": "Repository for the omnibar website",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "format": "npm run format:js && npm run format:md",
    "format:js": "prettier --write '{*.js,*.json,{src,__tests__}/**/*.ts{,x}}'",
    "format:md": "prettier --parser markdown --write '*.md'",
    "precommit": "lint-staged",
    "dev": "webpack-dev-server -d --inline",
    "start": "serve",
    "deploy": "now && now alias"
  },
  "lint-staged": {
    "{*.js,{src,__tests__}/**/*.ts{,x}}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/vutran/omnibar-www.git"
  },
  "keywords": [
    "omnibar",
    "search",
    "react",
    "component",
    "omnisearch"
  ],
  "author": "Vu Tran <vu@vu-tran.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vutran/omnibar-www/issues"
  },
  "homepage": "https://github.com/vutran/omnibar-www#readme",
  "devDependencies": {
    "@types/highlight.js": "^9.1.9",
    "@types/mathjs": "0.0.35",
    "@types/node": "^7.0.18",
    "@types/react": "^15.0.24",
    "@types/react-dom": "^15.5.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^0.28.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "mathjs": "^3.18.0",
    "now": "^5.3.0",
    "prettier": "^1.9.2",
    "style-loader": "^0.17.0",
    "ts-loader": "^2.0.3",
    "typescript": "^2.3.2",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "highlight.js": "^9.11.0",
    "omnibar": "^1.1.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "serve": "^5.1.5"
  },
  "now": {
    "alias": "omnibar"
  }
}
